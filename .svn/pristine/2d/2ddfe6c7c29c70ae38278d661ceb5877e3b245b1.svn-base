/* 设置删除后下一个元素上移的动画 */
.v-move {
  transition: all .8s ease;
}

.v-leave-active {
  /*position: fixed;*/
  animation: fadeOut .4s .2s ease both;
  -webkit-animation: fadeOut .4s .2s ease both;
  -moz-animation: fadeOut .4s .2s ease both;
  -ms-animation: fadeOut .4s .2s ease both;
}

