/*footer底部影子*/
.footer_box {
  position: relative;
  display: flex;
  justify-content: flex-end;
  height: 58px;
  width: 100%;
  align-items: center;
}
.footer_shadow::after {
  content: "";
  display: block;
  height: 5px;
  position: absolute;
  top: 0;
  right: 10px;
  left: 10px;
  background-image: url("/static/images/global/shadow_footbar.png");
  background-repeat: repeat-x;
  background-color: transparent;
}


.viewer-button {
  display: none;
}

/*工具弹框拖拽样式*/
.moveDilaog {
  opacity: 0;
  visibility: hidden !important;
  left: 999999px;
  top: 999999px;
  display: block !important;
}

.dialog1.showMoveDialog {
  visibility: visible;
  display: block !important;
  z-index: 9999;
  box-shadow: 0px 2px 6px rgba(0, 0, 0, 0.5);
}

/*音频播放器的修改*/

.aplayer-time-inner {
  color: transparent;
}

.aplayer-time-inner span {
  color: #858585;
}

span.aplayer-list-author {
  color: transparent !important;
}

span.aplayer-ptime {
  position: relative;
}

span.aplayer-ptime:after {
  position: absolute;
  content: '/';
  right: -7px;
}

.radio-custom-label, .checkbox-custom-label {
  position: relative;
}

.radio-custom, .checkbox-custom {
  opacity: 0;
  position: absolute;
}

.radio-custom, .radio-custom-label, .checkbox-custom, .checkbox-custom-label {
  display: inline-block;
  vertical-align: middle;
  cursor: pointer;
}

.checkbox-custom + .checkbox-custom-label:before, .radio-custom + .radio-custom-label:before {
  content: '';
  vertical-align: sub;
  display: inline-block;
  background: url("/static/images/global/checkbox.png") center 0px no-repeat;
  height: 20px;
  width: 20px;
}

.radio-custom:checked + .radio-custom-label:before {
  background: url("/static/images/global/checkbox.png") center -66px no-repeat;
}

.checkbox-custom:checked + .checkbox-custom-label:before {
  background: url("/static/images/global/checkbox.png")  center -66px no-repeat;
}
.radio-custom[disabled='disabled'],.radio-custom[disabled='disabled']+label{
  cursor: not-allowed;
}
.radio-custom[disabled='disabled']+label:before {
  opacity:0.5;
}
