/*等陆弹出框样式*/
.el-notification {
  background-color: rgba(0, 0, 0, .5) !important;
  border: 0 !important;
  z-index: 99999 !important;
  box-sizing: border-box;
}

.el-notification__title {
  color: #ccc !important;
}

.el-notification__content {
  color: #ccc !important;
}

.el-notification__content p {
  width: 100%;
  height: auto;
  word-wrap: break-word;
  word-break: break-all;
  overflow: hidden;
}

/*全局滚动条*/
::-webkit-scrollbar-track { /*滚动条里面轨道*/
  /*-webkit-box-shadow: inset 0 0 5px rgba(255, 255, 255, 1);*/
  border-radius: 0;
  background: transparent;
}

::-webkit-scrollbar { /*滚动条整体样式*/
  width: 8px; /*高宽分别对应横竖滚动条的尺寸*/
  height: 8px;
}

::-webkit-scrollbar-thumb { /*滚动条里面小方块*/
  /*border-radius: 5px;*/
  /*-webkit-box-shadow: inset 0 0 5px rgba(0,0,0,.7);*/
  background: rgba(0, 0, 0, .4);
}

/*合并的盒子 滚动条样式*/
section::-webkit-scrollbar { /*滚动条整体样式*/
  width: 6px; /*高宽分别对应横竖滚动条的尺寸*/
  height: 6px;
}

section::-webkit-scrollbar-thumb { /*滚动条里面小方块*/
  border-radius: 5px;
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 1);
  background: #29435C;
}

section::-webkit-scrollbar-track { /*滚动条里面轨道*/
  -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
  border-radius: 0;
  background: rgba(0, 0, 0, 0.1);
}

/*导航菜单滚动条样式*/
.navigationMenu::-webkit-scrollbar { /*滚动条整体样式*/
  width: 6px; /*高宽分别对应横竖滚动条的尺寸*/
  height: 6px;
}

.navigationMenu::-webkit-scrollbar-thumb { /*滚动条里面小方块*/
  /*border-radius: 5px;*/
  -webkit-box-shadow: inset 0 0 5px;
  background: rgba(255, 255, 255, .2);
}

.navigationMenu::-webkit-scrollbar-track { /*滚动条里面轨道*/
  -webkit-box-shadow: inset 0 0 5px rgba(255, 255, 255, .2);
  border-radius: 0;
  background: rgba(255, 255, 255, .2);
}

.el-popper[x-placement^=right] .popper__arrow::after {
  border-right-color: rgba(0, 0, 0, .89) !important;
  left: 0 !important;
}

