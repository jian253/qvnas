  cursor: pointer;
  padding-right: 20px;
}

.Overflow_icon {
  height: 32px;
  width: 32px;
  background: url("/static/images/desktop/hearder-sprite.png") 0 0;
}

.Overflow_icon:hover {
  background: url("/static/images/desktop/hearder-sprite.png") 0 -32px;
}

.Overflow_icon:active {
  background: url("/static/images/desktop/hearder-sprite.png") 0 -64px;
}

.Overflow_dropdown_img {
  display: block;
  width: 32px;
  height: 32px;
}

.Overflow_dropdown_item_box {
  display: flex;
  align-items: center;
  padding-bottom: 2px;
  padding-top: 2px;
}

.Overflow_dropdown_item {
  font-size: 14px;
  color: #0086E5;
  cursor: pointer;
  border-bottom: 1px solid #EBF0F5;
  padding: 0 10px;
}

.Overflow_dropdown_item:last-child {
  border-bottom: 0;
}

.Overflow_dropdown_item:hover {
  color: #505050;
}

.Overflow_dropdown_title {
  padding-left: 10px;
  display: inline-block;
  width: 166px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}


