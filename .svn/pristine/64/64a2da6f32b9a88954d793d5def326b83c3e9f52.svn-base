/*tabs样式*/
.el-tabs .el-tabs__content {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  /*flex: 1;*/
  /*display: flex;*/
}

.tabs.el-tabs {
  position: relative;
  height: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  /*display: flex;*/
  /*flex-direction: column;*/

}

.el-tabs__content {
  position: absolute;
  top: 32px;
  right: 0;
  left: 0;
  bottom: 0;
}

.el-tab-pane {
  height: 100%;
  width: 100%;
}
/*折叠表格展开的样式*/
tr.el-table__row.current-row.expanded+tr>.el-table__expanded-cell{
  background-color: #F5FAFF!important;
}
