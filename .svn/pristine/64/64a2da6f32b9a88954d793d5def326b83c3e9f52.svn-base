/*消息对话框样式*/
.el-drawer__body {
  /*overflow-y: auto;*/
  height: 100%;
}

.el-drawer {
  /*margin-top: 39px;*/
  /*box-shadow: -3px 0 5px rgba(0, 0, 0, .7) !important;*/
  box-shadow: -2px 5px 8px rgba(0, 0, 0, 0.5);
}

.el-drawer__wrapper {
  top: 39px;
}

.el-badge__content {
  background-color: #FA4B4B;
  border: 0;
}

.el-badge__content.is-fixed.is-dot {
  top: 17px;
  right: 8px;
}

.el-badge__content.is-dot {
  height: 9px;
  width: 9px;
}

.el-table::before {
  height: 0;
}

.el-tabs--card > .el-tabs__header .el-tabs__item {
  /*border: 0px solid transparent;*/
  /*border-left: 1px solid #D7E1EB;*/
  border-right: 1px solid #D7E1EB;
}

.el-tabs--card > .el-tabs__header .el-tabs__item.is-active {
  /*border: 0px solid transparent;*/
}

.el-tabs__nav div:nth-child(1) {
  border-left: 1px solid #D7E1EB !important;
}

.el-tabs--card > .el-tabs__header {
  border: 0px solid transparent;
  position: relative;
}

.el-tabs__header::before {
  overflow: unset;
  content: "";
  position: absolute;
  bottom: -1px;
  right: 0;
  left: 0;
  height: 3px;
  background-image: url("../images/tabBar/tab_shadow.png");
  /*background-color: #000;*/
  background-position: 0 -6px !important;
  background-repeat: repeat-x !important;
  background-size: 16px 10px !important;
}

.el-tabs__item.is-active::before {
  content: "";
  position: absolute;
  bottom: -11px;
  width: 18px;
  height: 8px;
  left: 50%;
  transform: translate(-50%);
  background-repeat: no-repeat;
  /*background-position: center 31px;*/
  background-image: url("/static/images/tabBar/tab_arrow.png");
}

.el-tabs__item.is-active {
  font-weight: bold;
  color: #0086E5;
}

.el-tabs--card > .el-tabs__header .el-tabs__nav {
  border: 0px solid transparent;
  height: 100%;
  display: flex;
  align-items: baseline;
}

.el-tabs--card > .el-tabs__header .el-tabs__item {
  border-left: 0px solid transparent;
}

.el-tabs__item {
  height: 24px;
  line-height: 24px;
  font-size: 12px;
}

.el-tabs__header {
  height: 34px;
}

.el-tabs__nav-wrap::after {
  height: 0;
  background-color: transparent;
}

.el-tabs__nav-wrap {
  height: 100%;
}

.el-tabs__nav-scroll {
  height: 100%;
}

.el-tabs--card > .el-tabs__header .el-tabs__nav > div {
  /*border-left: #E4e7ed;*/
}

