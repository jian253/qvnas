.el-tree-node__expand-icon {
  color: unset;
}

.el-table--border th {
  border-right: 1px solid #eee;
  border-top: 1px solid #D7E1EB;
  padding-top: 0px !important;
  padding-bottom: 0px !important;
}

.el-dropdown .el-dropdown__caret-button.el-button--default::before {
  background-color: #C8D2DC !important;
}

:focus {
  outline: 0;
}

/*搜索建议样式*/
.pathSearchListCalss li, .controlMenuSearch li {
  font-size: 12px;
  line-height: 28px
}

input.el-input__inner {
  font-size: 12px;
  box-shadow: inset 0px 1px 0px rgba(80, 90, 100, 0.08)
}

.menuSearch .el-input__icon.icon-chazhao2 {
  height: 26px !important;
  line-height: 26px !important;
}

.pathSearchListCalss li:hover {
  background-color: #0086e5;
  color: #fff;
}

.controlMenuSearch li:hover {
  background-color: #E6F5FF;
}

.pathSearchListCalss .el-autocomplete-suggestion__wrap, .controlMenuSearch .el-autocomplete-suggestion__wrap {
  padding: 5px;
}

.controlMenuSearch li {
  padding-left: 35px;
}

.controlMenuSearch li::before {
  content: "";
  position: absolute;
  left: 5px;
  top: 50%;
  height: 22px;
  transform: translate(0%, -50%);
  width: 20px;
  background: url("/static/images/desktop/ControlPanel_32.png") no-repeat;
  background-size: 20px;
}

.el-transfer-panel .el-transfer-panel__header .el-checkbox .el-checkbox__label {
  font-size: 13px;
}

