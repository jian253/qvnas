const state={
  poolName:null,
  poolDeleteId: null,
  isReset:0
};
const mutations={
  pooName(state,params){//保存要删除的池name
    state.poolName=params
  },
  //要删除的存储池id 保存
  pool_delete_id (state, id) {
    state.poolDeleteId = id
  },
  //重置存储空间面板的内容
  ResetStorage(state){
    state.isReset++
  },
};
const actions={};
export default {
  namespaced:true,
  state,
  mutations,
  actions
}
