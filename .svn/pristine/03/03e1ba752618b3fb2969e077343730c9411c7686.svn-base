//全局组件
import loadAnimation from "../components/customComponent/loadAnimation";
import dialog from "../components/customComponent/deleteMessageDialog";
import SwitchBtn from "../components/customComponent/SwitchBtn";
import ButtonStyle from "../components/customComponent/ButtonStyle";
import rightMenu from "../components/customComponent/rightMenu";
import Button from "../components/customComponent/Button";
import warningDialog from "../components/customComponent/warningDialog";
import dropdown from "../components/customComponent/dropdowns";
import carousel from "../components/customComponent/carousel";
import Tooltip from "../components/customComponent/Tooltip";
import lodding from "@/components/customComponent/lodding";
import carouselView from "@/components/customComponent/carouselView/carouselView";
function plugin(Vue) {
  if(plugin.installed) {
    return;
  }

  Vue.component('glLoadAnimation', loadAnimation);
  Vue.component('glDialog', dialog);
  Vue.component('glSwitchBtn', SwitchBtn);
  Vue.component('glButtonStyle',ButtonStyle)
  Vue.component('glRightMenu',rightMenu)
  Vue.component('glButton',Button)
  Vue.component('glWarningDialog',warningDialog)
  Vue.component('glDropdown',dropdown)
  Vue.component('glCarousel',carousel)
  Vue.component('glTooltip',Tooltip)
  Vue.component('glLoading',lodding)
  Vue.component('glCarouselView',carouselView)
}
export default plugin
